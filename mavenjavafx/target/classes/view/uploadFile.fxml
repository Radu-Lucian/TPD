<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="373.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="WindowController.UploadFileController">
   <children>
      <Button fx:id="selectFileButton" layoutX="30.0" layoutY="340.0" mnemonicParsing="false" text="Select file" onAction="#onSelectFileButtonClick"/>
      <Button fx:id="uploadButton" layoutX="233.0" layoutY="340.0" mnemonicParsing="false" text="Upload To Server" onAction="#onUploadButtonClick"/>
      <ListView fx:id="usersListView" layoutX="30.0" layoutY="112.0" prefHeight="200.0" prefWidth="311.0" />
      <Label layoutX="30.0" layoutY="88.0" text="Select users you want to give right" />
      <Label layoutX="30.0" layoutY="47.0" text="File to upload:" />
      <Label fx:id="fileStatusLabel" layoutX="121.0" layoutY="47.0" text="not loaded" />
   </children>
</AnchorPane>
