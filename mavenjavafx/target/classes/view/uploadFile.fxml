      <Label layoutX="30.0" layoutY="47.0" text="File to upload:" />
      <Label fx:id="fileStatusLabel" layoutX="121.0" layoutY="47.0" text="not loaded" />
      <TableView fx:id="usersTableView" layoutX="29.0" layoutY="111.0" prefHeight="219.0" prefWidth="308.0">
        <columns>
          <TableColumn prefWidth="75.0" text="C1" />
          <TableColumn prefWidth="75.0" text="C2" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
